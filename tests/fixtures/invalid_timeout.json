{
  "tools": {
    "compile": {
      "description": "Compile source code",
      "command": "gcc",
      "cwd": "/tmp",
      "timeout": {
        "total": -1000,
        "inactive": 10000
      },
      "streamOutput": true,
      "outputProcessing": {
        "extractor": {
          "type": "gcc",
          "patterns": {
            "errors": ["error:"],
            "warnings": ["warning:"]
          }
        },
        "formatOutput": true
      },
      "resultFormat": {
        "successMessage": "Compilation successful",
        "errorMessage": "Compilation failed"
      }
    }
  }
}
